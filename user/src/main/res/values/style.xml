<?xml version="1.0" encoding="utf-8"?>
<resources>


    <!--公用分割线样式-->
    <style name="mine_tab">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_50</item>
        <item name="android:background">@color/white</item>
    </style>


    <style name="mine_order">

        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:paddingTop">@dimen/dp_5</item>
        <item name="android:drawablePadding">@dimen/dp_8</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/font_size_12</item>
        <item name="android:textColor">@color/font_black_light</item>

    </style>


</resources>